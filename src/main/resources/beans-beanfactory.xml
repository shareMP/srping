<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- 通过FactoryBean配置bean的实例
		class:FactoryBean的类名
		property:factorybean的属性
		返回的值是：FactoryBean  getObject方法返回的实例
	 -->
	<bean id="car" class="com.mrl.spring.beans.factorybean.CarFactoryBean">
		<property name="brand" value="Benz"></property>
	</bean>
	
</beans>
